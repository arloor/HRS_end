<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.media.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="475.0" styleClass="background-default" stylesheets="@DarkTheme.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="presentation.customer.view.LivedHotelController">
   <children>
      <TableView fx:id="livedHotelTable" layoutX="3.0" layoutY="23.0" prefHeight="332.0" prefWidth="415.0" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0">
         <columns>
            <TableColumn fx:id="hotelNameColumn" prefWidth="125.0" text="酒店名称" />
            <TableColumn fx:id="cityColumn" prefWidth="39.0" text="城市" />
            <TableColumn fx:id="areaColumn" prefWidth="125.0" text="商圈" />
            <TableColumn fx:id="levelColumn" prefWidth="41.0" text="星级" />
            <TableColumn fx:id="scoreColumn" prefWidth="39.0" text="评分" />
            <TableColumn fx:id="lowestPriceColumn" prefWidth="70.0" text="最低价格" />
            <TableColumn fx:id="addressColumn" minWidth="3.0" prefWidth="125.0" text="地址" />
         </columns>
      </TableView>
       <Button layoutX="270.0" layoutY="363.0" mnemonicParsing="false" onAction="#setHotelOrderButton"
               onMouseClicked="#setHotelOrderButton" prefWidth="75.0" styleClass="button-default" text="查看订单"/>
       <Button layoutX="370.0" layoutY="363.0" mnemonicParsing="false" onAction="#orderThis" prefHeight="23.0"
               prefWidth="75.0" styleClass="button-default" text="预订"/>
   </children>
</AnchorPane>
